<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.correcto2">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="400.0" prefWidth="500.0">
         <children>
            <ImageView fx:id="correcto" fitHeight="238.0" fitWidth="330.0" layoutX="131.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Imagenes/ventaja.png" />
               </image>
            </ImageView>
            <Label layoutX="176.0" layoutY="283.0" prefHeight="54.0" prefWidth="148.0" text="CORRECTO" textFill="#19eb0e">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <Button fx:id="continuar" layoutX="348.0" layoutY="324.0" mnemonicParsing="false" onAction="#btn_continuar" prefHeight="27.0" prefWidth="96.0" text="Continuar" textFill="#1b1a1a" />
            <Button fx:id="Salir" layoutX="53.0" layoutY="324.0" mnemonicParsing="false" onAction="#btn_salir" prefHeight="27.0" prefWidth="78.0" text="Salir" />
         </children>
      </Pane>
   </children>
</GridPane>
